{
  "name": "bsf-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:local": "APP_ENV=local next build",
    "build:staging": "cp .env.staging .env.production.local && APP_ENV=staging next build",
    "build:production": "APP_ENV=production next build",
    "deploy:local": "pnpm build:local && pm2 startOrRestart ecosystem.config.js --env local",
    "deploy:staging": "pnpm build:staging && pm2 startOrRestart ecosystem.config.js --env staging",
    "deploy:production": "pnpm build:production && pm2 startOrRestart ecosystem.config.js --env production"
  },
