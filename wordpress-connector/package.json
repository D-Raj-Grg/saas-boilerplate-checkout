{
  "name": "surecrm",
  "version": "0.0.1",
  "description": "A powerful CRM connector plugin for WordPress",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack --mode production",
    "build:css": "tailwindcss -i ./assets/tailwind.css -o ./assets/css/tailwind.css --minify",
    "watch:js": "webpack --watch --mode development",
    "watch:css": "tailwindcss -i ./assets/tailwind.css -o ./assets/css/tailwind.css --watch",
    "dev": "concurrently \"npm run watch:js\" \"npm run watch:css\"",
    "test:e2e": "cd tests/e2e && npm test --",
    "package": "./build.sh",
    "zip": "./build.sh --build-assets"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "crm",
    "connector",
    "integration"
  ],
  "author": "SureCRM",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-primitive": "^1.0.0",
    "@radix-ui/react-progress": "^1.0.0",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "chart.js": "^4.3.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "react-select": "^5.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.1.12",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "overrides": {
    "inflight": "^2.0.0",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@latest",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@latest",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.2.19"
  }
}