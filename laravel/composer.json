{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.33",
        "laravel/nightwatch": "^1.13",
        "laravel/octane": "^2.12",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.1",
        "laravel/socialite": "^5.23",
        "laravel/tinker": "^2.10.1",
        "posthog/posthog-php": "^3.7",
        "predis/predis": "^3.0",
        "propaganistas/laravel-disposable-email": "^2.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.82",
        "knuckleswtf/scribe": "^5.2",
        "larastan/larastan": "^3.5",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "test:pest": [
            "@putenv DB_CONNECTION=sqlite",
            "@putenv DB_DATABASE=:memory:",
            "@putenv APP_KEY=base64:2fl+Ktvkdphvz06+KFSjrLIxw9OqSKm/CvQjt5C5w3c=",
            "./vendor/bin/pest"
        ],
        "test:coverage-html": [
            "@putenv DB_CONNECTION=sqlite",
            "@putenv DB_DATABASE=:memory:",
            "@putenv APP_KEY=base64:2fl+Ktvkdphvz06+KFSjrLIxw9OqSKm/CvQjt5C5w3c=",
            "./vendor/bin/pest --coverage-html coverage"
        ],
        "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "format:check": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
        "lint": "./vendor/bin/pint",
        "lint:check": "./vendor/bin/pint --test",
        "analyze": "./vendor/bin/phpstan analyze --memory-limit=2G",
        "analyze:baseline": "./vendor/bin/phpstan analyze --generate-baseline",
        "check": [
            "@format:check",
            "@lint:check",
            "@analyze",
            "@test:pest"
        ],
        "docs:generate": "@php artisan scribe:generate"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}